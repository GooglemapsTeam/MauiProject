    <ShellContent
        ContentTemplate="{DataTemplate local:SixthSurveyPage}"
        Route="SixthSurveyPage"/>

    <ShellContent
        ContentTemplate="{DataTemplate local:SeventhSurveyPage}"
        Route="SeventhSurveyPage"/>

    <ShellContent
        ContentTemplate="{DataTemplate local:ProfilePage}"
        Route="ProfilePage"/>

    <ShellContent
        ContentTemplate="{DataTemplate local:FavouritePage}"
        Route="FavouritePage"/>

    <ShellContent
        ContentTemplate="{DataTemplate local:MainPage}"
